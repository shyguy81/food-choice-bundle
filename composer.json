{
  "name": "shyguy81/food-choice-bundle",
  "description": "Symfony bundle to integrate shyguy81/food-choice-core into a Symfony app",
  "type": "symfony-bundle",
  "license": "MIT",
  "homepage": "https://github.com/shyguy81/food-choice-bundle",
  "authors": [
    {
      "name": "shyguy81"
    }
  ],
  "require": {
    "php": "^8.1",
    "shyguy81/food-choice-core": "^1.0 || dev-main",
    "symfony/framework-bundle": "^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Shyguy\\FoodChoiceBundle\\": "src/"
    }
  },
  "keywords": [
    "symfony",
    "bundle",
    "food",
    "php"
  ],
  "support": {
    "issues": "https://github.com/shyguy81/food-choice-bundle/issues",
    "source": "https://github.com/shyguy81/food-choice-bundle.git"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  }
}
